{
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "extensions": [".js", ".jsx", ".ts", ".tsx"],
        "alias": {
          "@Components/*": ["Components/*"],
          "@Components": ["Components"],
          "@Services/*": ["Services/*"],
          "@Services": ["Services"],
          "@Stores/*": ["Stores/*"],
          "@Stores": ["Stores"],
          "@Types/*": ["Types/*"],
          "@Types": ["Types"],
          "@Hooks/*": ["Hooks/*"],
          "@Hooks": ["Hooks"],
          "@Pages/*": ["Pages/*"],
          "@Pages": ["Pages"]
        }
      }
    ]
  ]
}
